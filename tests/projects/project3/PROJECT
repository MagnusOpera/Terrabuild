extensions:
  - (dotnet): project3.csproj
  - (docker): Dockerfile

steps:
  build:
    - (dotnet): build
      configuration: $(configuration)

  publish:
   - (dotnet): publish
     configuration: $(configuration)

  docker:
   - (docker): build
     configuration: $(configuration)

  push:
   - (docker): push
     tag: ghcr.io/magnus-opera/project3:666
