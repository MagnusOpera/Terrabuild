builders:
  dotnet:
  docker:
    use: docker
    with: Dockerfile
    parameters:
      image: ghcr.io/magnus-opera/project3
      arguments:

steps:
  build:
    - (dotnet): build

  publish:
    - (dotnet): publish

  docker:
    - (docker): build
      arguments:
        configuration: $(configuration)

  push:
    - (docker): push
      tag: 666
